{
  "name": "dcerialize",
  "description": "Easy serialization through ES7/Typescript annotations",
  "keywords": [
    "serialize",
    "typescript",
    "es7",
    "annotations",
    "decoration",
    "newtonsoft",
    "decorators",
    "cycle",
    "cyclic"
  ],
  "author": "Matt Weichselbaum & Diad√®me Ingenierie",
  "license": "MIT",
  "repository": "https://github.com/Diademe/dcerialize",
  "version": "3.6.6",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --project src/tsconfig.app.json",
    "test": "jest  --config spec/jest.config.js --colors",
    "coverage": "jest --config spec/jest.config.js --coverage",
    "lint": "npm run lint:app && npm run lint:test",
    "lint:app": "eslint -c .eslintrc.js --ext .ts src",
    "lint:test": "eslint -c .eslintrc.js --ext .ts spec"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-plugin-jsdoc": "^32.3.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  }
}
