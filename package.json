{
  "name": "dcerialize",
  "description": "Easy serialization through ES7/Typescript annotations",
  "keywords": [
    "serialize",
    "typescript",
    "es7",
    "annotations",
    "decoration",
    "newtonsoft",
    "decorators"
  ],
  "author": "Matt Weichselbaum & Diad√®me Ingenierie",
  "license": "MIT",
  "repository": "https://github.com/Diademe/dcerialize",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "pwd && ls && tsc --project src/tsconfig.json",
    "test": "jest",
    "install": "npm run build",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "tslint": "tslint --config ./tslint.json -p src/tsconfig.json -p spec/tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "ts-jest": "^22.4.5",
    "tslint": "^5.10.0"
  },
  "jest": {
    "testRegex": "/spec/.*\\.spec\\.([tj]sx?)$",
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "typescript": "^2.8.3"
  }
}
